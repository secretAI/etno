{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
  "ecmaVersion": "latest",
  "sourceType": "module"
  },
  "plugins": [
  "@typescript-eslint"
  ],
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "double"
    ],
    "semi": [
      1,
      "always"
    ]
  }
}